{
  "name": "LookinShared",
  "version": "1.2.8",
  "summary": "The shared files between client and server side of Lookin.",
  "description": "Embed this framework into your iOS project to enable Lookin mac app.",
  "homepage": "https://lookin.work",
  "license": "GPL-3.0",
  "authors": {
    "Li Kai": "lookin@lookin.work"
  },
  "platforms": {
    "ios": "9.0",
    "tvos": "9.0",
    "osx": "10.14",
    "visionos": "1.0"
  },
  "source": {
    "git": "https://github.com/QMUI/LookinServer.git",
    "tag": "1.2.8"
  },
  "requires_arc": true,
  "source_files": [
    "Src/Main/Shared/**/*",
    "Src/Base/**/*"
  ],
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) SHOULD_COMPILE_LOOKIN_SERVER=1"
  }
}
